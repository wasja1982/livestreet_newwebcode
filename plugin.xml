<?xml version="1.0" encoding="UTF-8"?>
<plugin>
        <name>
                <lang name="default">New Webcode</lang>
        </name>
        <author>
                <lang name="default">Artem Soshnikov, Alexandr Verenik</lang>
                <lang name="russian">Артем Сошников, Александр Вереник</lang>
        </author>
        <homepage>https://github.com/wasja1982/livestreet_newwebcode</homepage>
        <version>1.0.2</version>
        <requires>
                <livestreet>1.0.3</livestreet>
                <plugins>
                        <plugin>configengine</plugin>
                </plugins>
        </requires>
        <description>
                <lang name="default">Easy way to add counters (Google Analitics, etc.) codes</lang>
                <lang name="russian">Простое добавление счетчиков (Яндекс.Метрика, Google Analytics и др.) без редактирования шаблонов</lang>
        </description>
        <delegate>
                <module></module>
                <action></action>
                <template></template>
                <entity></entity>
        </delegate>
</plugin>